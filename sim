v0x562a6fdf5840_0 .net "q", 0 0, v0x562a6fdf52f0_0;  1 drivers
S_0x562a6fda4cf0 .scope module, "DUT" "simple_module" 3 13, 4 3 0, S_0x562a6fdded90;
 .timescale -9 -12;
    .port_info 0 /INPUT 1 "clk";
    .port_info 1 /INPUT 1 "en";
    .port_info 2 /INPUT 1 "d";
    .port_info 3 /OUTPUT 1 "q";
v0x562a6fda67f0_0 .net "clk", 0 0, v0x562a6fdf5430_0;  1 drivers
v0x562a6fda6c00_0 .net "d", 0 0, v0x562a6fdf55b0_0;  1 drivers
v0x562a6fdce970_0 .net "en", 0 0, v0x562a6fdf5680_0;  1 drivers
v0x562a6fdf52f0_0 .var "q", 0 0;
E_0x562a6fda53e0 .event posedge, v0x562a6fda67f0_0;
    .scope S_0x562a6fda4cf0;
T_0 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x562a6fdf52f0_0, 0, 1;
    %end;
    .thread T_0;
    .scope S_0x562a6fda4cf0;
T_1 ;
    %wait E_0x562a6fda53e0;
    %load/vec4 v0x562a6fdce970_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_1.0, 8;
    %load/vec4 v0x562a6fda6c00_0;
    %assign/vec4 v0x562a6fdf52f0_0, 0;
    %jmp T_1.1;
T_1.0 ;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x562a6fdf52f0_0, 0;
T_1.1 ;
    %jmp T_1;
    .thread T_1;
    .scope S_0x562a6fdded90;
T_2 ;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x562a6fdf54f0_0, 0, 32;
    %end;
    .thread T_2, $init;
    .scope S_0x562a6fdded90;
T_3 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x562a6fdf5430_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x562a6fdf5680_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x562a6fdf55b0_0, 0, 1;
    %vpi_func 3 24 "$fopen" 32, "test_result.txt", "w" {0 0 0};
    %store/vec4 v0x562a6fdf5750_0, 0, 32;
    %end;
    .thread T_3;
    .scope S_0x562a6fdded90;
T_4 ;
T_4.0 ;
    %delay 10000, 0;
    %load/vec4 v0x562a6fdf5430_0;
    %inv;
    %store/vec4 v0x562a6fdf5430_0, 0, 1;
    %jmp T_4.0;
    %end;
    .thread T_4;
    .scope S_0x562a6fdded90;
T_5 ;
    %wait E_0x562a6fda53e0;
    %load/vec4 v0x562a6fdf54f0_0;
    %addi 1, 0, 32;
    %assign/vec4 v0x562a6fdf54f0_0, 0;
    %load/vec4 v0x562a6fdf54f0_0;
    %cmpi/e 10, 0, 32;
    %jmp/0xz  T_5.0, 4;
    %vpi_call/w 3 32 "$finish" {0 0 0};
T_5.0 ;
    %jmp T_5;
    .thread T_5;
    .scope S_0x562a6fdded90;
T_6 ;
    %wait E_0x562a6fda53e0;
    %load/vec4 v0x562a6fdf55b0_0;
    %assign/vec4 v0x562a6fdf5680_0, 0;
    %load/vec4 v0x562a6fdf5680_0;
    %inv;
    %assign/vec4 v0x562a6fdf55b0_0, 0;
    %jmp T_6;
    .thread T_6;
    .scope S_0x562a6fdded90;
T_7 ;
    %wait E_0x562a6fda53e0;
    %vpi_call/w 3 41 "$write", "clk:%d en:%b d:%b q:%b\012", v0x562a6fdf54f0_0, v0x562a6fdf5680_0, v0x562a6fdf55b0_0, v0x562a6fdf5840_0 {0 0 0};
    %vpi_call/w 3 43 "$fwrite", v0x562a6fdf5750_0, "clk:%d en:%b d:%b q:%b\012", v0x562a6fdf54f0_0, v0x562a6fdf5680_0, v0x562a6fdf55b0_0, v0x562a6fdf5840_0 {0 0 0};
    %jmp T_7;
    .thread T_7;
